{
  "name": "bmore-api",
  "version": "3.0.0",
  "description": "",
  "main": "index.js",
  "scripts": {
    "make-me-pretty": "prettier-eslint \"**/*.js\" --write",
    "test": "NODE_ENV=test NODE_PATH=$NODE_PATH:. nyc --reporter=text-summary --reporter=lcov --report-dir=test/reports/unit/ mocha test/unit",
    "func-test": "NODE_ENV=test NODE_PATH=$NODE_PATH:. nyc --reporter=text-summary --reporter=lcov --report-dir=test/reports/func/ mocha test/func/",
    "lint": "eslint . --fix",
    "serve": "nodemon --watch . --ignore ./node_modules -x \"npm start\"",
    "start": "NODE_PATH=$NODE_PATH:. node server",
    "seed": "sequelize db:migrate && sequelize db:seed:all"
  },
  "lint-staged": {
    "**/*.js": [
      "npm run make-me-pretty",
      "npm run lint",
      "git add"
    ]
  },
  "husky": {
    "hooks": {
      "pre-commit": "lint-staged",
      "pre-push": "auditmated"
    }
  },
  "author": "Bob Evans",
  "license": "MIT",
  "dependencies": {
    "@google/maps": "^0.5.5",
    "bluebird": "^3.5.2",
    "body-parser": "^1.19.0",
    "config": "^3.2.2",
    "cors": "^2.8.1",
    "declare-validator": "^1.1.0",
    "express": "^4.14.0",
    "lodash": "^4.17.15",
    "mysql2": "^1.6.1",
    "on-headers": "^1.0.1",
    "response-time": "^2.3.1",
    "sequelize": "^5.19.4",
    "sequelize-cli": "^5.5.1",
    "uuid": "^3.3.2",
    "winston": "^3.1.0"
  },
  "devDependencies": {
    "auditmated": "^0.1.2",
    "chai": "^4.2.0",
    "chai-as-promised": "^7.1.1",
    "codecov": "^3.6.1",
    "eslint": "^6.4.0",
    "eslint-config-sparkpost": "^1.10.1",
    "husky": "^3.0.5",
    "lint-staged": "^9.3.0",
    "mocha": "^5.2.0",
    "nodemon": "^1.18.4",
    "nyc": "^14.1.1",
    "prettier-eslint-cli": "^4.7.1",
    "proxyquire": "^2.1.0",
    "sinon": "^7.5.0",
    "sinon-chai": "^3.2.0",
    "supertest": "^3.3.0"
  }
}
